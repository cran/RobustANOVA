Package: RobustANOVA
Type: Package
Title: Robust One-Way ANOVA Tests under Heteroscedasticity and
        Nonnormality
Version: 0.2.0
Authors@R: c(person("Gamze", "Guven", role = c("aut", "cre"),
                     email = "gamzeguven@ogu.edu.tr"),
              person("Sukru", "Acitas", role = c("aut"),
